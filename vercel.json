{
    "version": 2,
    "builds": [
        {
            "src": "/public/index.php",
            "use": "@vercel/php@latest"
        }
    ],
    "routes": [
        {
            "src": "/(css|js|images|fonts|favicon.ico)(.*)",
            "dest": "/public/$1$2"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://nama-proyek-anda.vercel.app",
        "APP_KEY": "",
        "DB_CONNECTION": "pgsql",
        "DB_HOST": "",
        "DB_PORT": "",
        "DB_DATABASE": "",
        "DB_USERNAME": "",
        "DB_PASSWORD": "",
        "SESSION_DRIVER": "file",
        "CACHE_DRIVER": "file",
        "LOG_CHANNEL": "stderr",
        "SESSION_LIFETIME": "120"
    }
}
